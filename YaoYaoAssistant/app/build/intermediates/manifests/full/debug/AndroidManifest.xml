<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ssdut411.yaoyaoassistant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.ssdut411.yaoyaoassistant.application.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ssdut411.yaoyaoassistant.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ssdut411.yaoyaoassistant.activity.CreateDetailsActivity" />
        <activity android:name="com.ssdut411.yaoyaoassistant.activity.DetailsActivity" />
        <activity android:name="com.ssdut411.yaoyaoassistant.activity.CreateAccountActivity" />
        <activity android:name="com.ssdut411.yaoyaoassistant.activity.TransferActivity" />
    </application>

</manifest>